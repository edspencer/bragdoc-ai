{
  "name": "bragdoc-ai",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.5.0",
  "workspaces": ["apps/*", "packages/*"],
  "scripts": {
    "setup": "pnpm install && pnpm db:generate && pnpm db:push",
    "dev": "turbo dev",
    "dev:all": "turbo dev",
    "dev:web": "turbo dev --filter=@bragdoc/web",
    "dev:marketing": "turbo dev --filter=@bragdoc/marketing",
    "dev:web-only": "turbo dev --filter=@bragdoc/web --filter=@bragdoc/database",
    "dev:marketing-only": "turbo dev --filter=@bragdoc/marketing",
    "build": "turbo build",
    "build:web": "turbo build --filter=@bragdoc/web",
    "build:marketing": "turbo build --filter=@bragdoc/marketing",
    "test:setup": "pnpm --filter=@bragdoc/database test:setup",
    "test": "turbo test",
    "test:watch": "turbo test:watch",
    "test:cli": "turbo test:cli",
    "eval": "npx braintrust eval ./evals/**/eval.ts",
    "lint": "turbo lint",
    "lint:fix": "turbo lint --filter=@bragdoc/web && turbo lint --filter=@bragdoc/marketing",
    "format": "turbo format",
    "db:generate": "turbo db:generate --filter=@bragdoc/database",
    "db:studio": "turbo db:studio --filter=@bragdoc/database",
    "db:push": "turbo db:push --filter=@bragdoc/database",
    "db:pull": "drizzle-kit pull",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "prepare": "husky"
  },
  "dependencies": {
    "@ai-sdk/deepseek": "^0.2.16",
    "@auth/core": "^0.41.0",
    "@auth/drizzle-adapter": "^1.11.0",
    "@openrouter/ai-sdk-provider": "^0.7.5",
    "@react-email/components": "^0.5.7",
    "@react-email/render": "^1.4.0",
    "@repo/typescript-config": "workspace:*",
    "@types/jsdom": "^21.1.7",
    "@vercel/postgres": "^0.10.0",
    "autoevals": "^0.0.131",
    "bcrypt-ts": "^5.0.3",
    "braintrust": "^0.4.7",
    "bright": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.6",
    "embla-carousel-react": "^8.6.0",
    "feed": "^4.2.2",
    "form-data": "^4.0.4",
    "global-jsdom": "^26.0.0",
    "gray-matter": "^4.0.3",
    "jsdom": "^26.1.0",
    "mailgun.js": "^10.4.0",
    "mdx": "^0.3.1",
    "nanoid": "^5.1.6",
    "next-auth": "5.0.0-beta.29",
    "next-mdx-remote": "^5.0.0",
    "postgres": "^3.4.7",
    "react-day-picker": "8.10.1",
    "react-share": "^5.2.2",
    "vaul": "^1.1.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/cli": "^2.29.7",
    "@eslint/js": "^9.38.0",
    "@shadcn/ui": "^0.0.4",
    "@swc/jest": "^0.2.39",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/d3-scale": "^4.0.9",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.9.1",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.5",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.33",
    "eslint-config-prettier": "^9.1.2",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "estree-walker": "2.0.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.2.5",
    "node-mocks-http": "^1.17.2",
    "puppeteer": "^24.26.0",
    "server-cli-only": "^0.3.2",
    "shadcn": "^2.10.0",
    "tiktoken": "^1.0.22",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "turbo": "^2.5.8",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "pnpm": {
    "overrides": {
      "estree-walker": "2.0.2"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,jsonc}": "biome format --write --files-ignore-unknown=true"
  }
}
