{
  "name": "@bragdoc/marketing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "start": "next start -p 3001",
    "lint": "next lint && biome lint --write --unsafe app/ components/ lib/ hooks/",
    "lint:fix": "next lint --fix && biome lint --write --unsafe app/ components/ lib/ hooks/",
    "format": "biome format --write app/ components/ lib/ hooks/",
    "build": "next build",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "dependencies": {
    "@next/mdx": "latest",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-popover": "1.1.4",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-tabs": "1.1.2",
    "@radix-ui/react-toast": "1.2.4",
    "@radix-ui/react-toggle": "1.1.1",
    "@radix-ui/react-tooltip": "1.1.6",
    "@vercel/analytics": "latest",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "latest",
    "gray-matter": "latest",
    "lucide-react": "^0.454.0",
    "next": "15.5.4",
    "next-mdx-remote": "latest",
    "next-themes": "latest",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^2.5.5"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^1.9.1",
    "@tailwindcss/postcss": "^4.1.9",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5",
    "tailwindcss": "^4.1.9",
    "tw-animate-css": "1.3.3",
    "typescript": "^5",
    "wrangler": "^4.40.3"
  }
}
