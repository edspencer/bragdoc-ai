{
  "name": "@bragdoc/cli",
  "version": "0.1.2",
  "description": "CLI tool for managing your brag document",
  "main": "dist/index.js",
  "bin": {
    "bragdoc": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "scope": "@bragdoc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edspencer/bragdoc-ai.git",
    "directory": "cli"
  },
  "keywords": [
    "brag-document",
    "cli",
    "career",
    "achievements"
  ],
  "author": "Ed Spencer",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "biome lint --write --unsafe src/",
    "format": "biome format --write src/",
    "link-cli": "pnpm link --global",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish"
  },
  "dependencies": {
    "@bragdoc/config": "workspace:*",
    "@types/yaml": "^1.9.7",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "node-fetch": "^2.7.0",
    "open": "^10.1.0",
    "winston": "^3.17.0",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.11",
    "@types/jest": "^29.5.11",
    "@types/mock-fs": "^4.13.4",
    "@types/node": "^20.10.6",
    "@types/node-fetch": "^2.6.10",
    "jest": "^29.7.0",
    "mock-fs": "^5.4.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.3.3"
  }
}
